{
  "_comment": "Example package.json for Node.js projects",
  "name": "github-workflows-example",
  "version": "1.0.0",
  "description": "Example project demonstrating GitHub workflows",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "build": "webpack --mode production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "lint:css": "stylelint src/**/*.css",
    "format": "prettier --write src/**/*.{js,css,md}",
    "format:check": "prettier --check src/**/*.{js,css,md}"
  },
  "keywords": ["github-actions", "ci-cd", "automation"],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "jest": "^29.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "stylelint": "^15.0.0",
    "nodemon": "^3.0.0",
    "webpack": "^5.0.0"
  },
  "dependencies": {
    "express": "^4.18.0"
  }
}
